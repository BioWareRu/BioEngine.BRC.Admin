        "@angular/language-service": "8.2.14",
        "@types/node": "14.14.19",
        "@types/object-path": "0.11.0",
        "@types/twitter-for-web": "0.0.1",
        "@types/url-parse": "1.4.3",
        "@types/uuid": "8.3.0",
        "codelyzer": "6.0.0",
        "node-sass": "4.14.1",
        "ts-node": "8.10.2",
        "tslint": "5.20.1",
        "tslint-consistent-codestyle": "1.16.0",
        "typescript": "3.5.3",
        "webpack-bundle-analyzer": "3.8.0"
    }
