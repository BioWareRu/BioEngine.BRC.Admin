{
    "name": "bioengine-admin",
    "version": "3.0.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BioWareRu/BioEngine.Admin.git"
    },
    "bugs": {
        "url": "https://github.com/BioWareRu/BioEngine.Admin/issues"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open --proxy-config proxy.conf.json",
        "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false --proxy-config proxy.conf.json",
        "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false --proxy-config proxy.conf.json",
        "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev",
        "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
        "build-prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
        "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
        "lint": "ng lint",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json"
    },
    "dependencies": {
        "@angular/animations": "^7.0.4",
        "@angular/cdk": "^7.0.4",
        "@angular/common": "^7.0.4",
        "@angular/compiler": "^7.0.4",
        "@angular/core": "^7.0.4",
        "@angular/flex-layout": "^7.0.0-beta.19",
        "@angular/forms": "^7.0.4",
        "@angular/http": "^7.0.4",
        "@angular/material": "^7.0.4",
        "@angular/material-moment-adapter": "^7.0.4",
        "@angular/platform-browser": "^7.0.4",
        "@angular/platform-browser-dynamic": "^7.0.4",
        "@angular/router": "^7.0.4",
        "angular-oauth2-oidc": "5.0.2",
        "angular-tree-component": "^8.0.0",
        "ckeditor": "^4.11.1",
        "class-transformer": "^0.2.0",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.5.7",
        "hammerjs": "^2.0.8",
        "moment": "2.22.2",
        "ng2-ckeditor": "^1.2.2",
        "ng4-validators": "^6.0.1",
        "ngx-input-file": "^7.0.0",
        "ngx-moment": "3.2.0",
        "object-path": "^0.11.4",
        "perfect-scrollbar": "^1.4.0",
        "rxjs": "^6.3.3",
        "rxjs-compat": "^6.3.3",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.11.2",
        "@angular/cli": "^7.0.6",
        "@angular/compiler-cli": "^7.0.4",
        "@angular/language-service": "^7.0.4",
        "@angularclass/hmr": "^2.1.3",
        "@types/node": "^10.12.9",
        "@types/object-path": "0.9.29",
        "codelyzer": "^4.5.0",
        "node-sass": "^4.10.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "webpack-bundle-analyzer": "^3.0.3"
    }
}
