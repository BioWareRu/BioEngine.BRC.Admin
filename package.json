    "@angularclass/hmr": "^2.1.3",
    "@types/node": "^12.0.3",
    "@types/object-path": "0.11.0",
    "@types/twitter-for-web": "0.0.1",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^3.4.4",
    "codelyzer": "^5.0.1",
    "node-sass": "^4.12.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-consistent-codestyle": "1.15.1",
    "typescript": "^3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
